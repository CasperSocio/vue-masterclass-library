  },
  "license": "MIT",
  "keywords": [
    "component",
    "library",
    "vue"
  ],
  "scripts": {
    "build": "vite build",
    "build:storybook": "storybook build -o dist-web",
    "deploy": "firebase deploy --only hosting:lib",
