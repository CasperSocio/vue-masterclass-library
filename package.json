    "prettier": "^2.8.4",
    "sass": "^1.55.0",
    "storybook": "^7.0.0-rc.0",
    "storybook-dark-mode": "^3.0.0-canary.191.4282.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.1.0",
    "vue-loader": "^17.0.1",
    "vue-tsc": "^1.2.0"
